	
<%= form_for @member.room do |f| %>
	<%= f.label :room_name %><br>
	<%= f.text_area :room_name %><br>
	<%= f.label :rent %><br>
	<%= f.text_area :rent %><br>

	<%= f.fields_for @member do |builder| %>
	  <%= builder.label :name, "Member name" %><br>
      <%= builder.text_field :name %><br>
	<% end %>

	<%= f.submit "Submit" %>
<% end %>